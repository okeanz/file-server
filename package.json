{
  "name": "file-server",
  "version": "1.0.0",
  "packageManager": "npm@10.9.2",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:build": "node ./dist/server.min.js",
    "build": "node esbuild.config.mjs && copyfiles -u 1 src/config.json dist/",
    "build:run": "npm run build && npm run start:build"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "chokidar": "^4.0.3",
    "express": "^5.1.0",
    "morgan": "^1.10.1",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.25.9",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2"
  }
}
